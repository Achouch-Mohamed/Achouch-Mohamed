<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Mohamed Achouch</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/all.min.csscss" />
    <link rel="stylesheet" href="~/css/normalize.css" />
    <link / rel="stylesheet" href="~/js/site.js">
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />
    <!-- font google -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto+Mono:ital,wght@0,100;0,200;0,300;0,400;0,500;1,100;1,200;1,300;1,400&display=swap" rel="stylesheet">
</head>
<body>
 <div class="container">
    <header id="header">       
            <!-- header menu -->
            <!-- Start header -->
            <div class="header">
                <div id="logo"><span>.</span>@ViewData["Title"]<span>()</span></div>
                <i class="fas fa-ellipsis-v" id="btnopen"></i>
                <i class="fas fa-times" id="btnclose"></i>
                <div class="nav">
                    <ul class="list-menu">
                        <li><a href="./home.html">.is()</a></li>
                        <li><a asp-action="Work" asp-controller="Home">.work()</a></li>
                        <li><a asp-action="About" asp-controller="Home">.about()</a></li>
                        <li><a asp-action="Contact" asp-controller="Home">.contact()</a></li>
                        <li><a href="https://www.linkedin.com/in/marwane-ouardi/" target="balanc" class="icon" style="color:#0e1212;"><i class="fab fa-linkedin-in"></i></a></li>
                        <li><a href="#" class="icon" style="color:#0e1212;"><i class="fab fa-github-square"></i></a></li>
                    </ul>
                </div>
            </div>
            <!-- End header -->
       
    </header>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>        
    </div>
    <footer class="footer">
        <div class="social-media">
            <i class="icon-footer fab fa-linkedin-in"></i>
            <i class="icon-footer fab fa-github-square"> </i>
            <i class="icon-footer fab fa-facebook-square"></i>
            <i class="icon-footer fab fa-instagram-square"></i>
        </div>
        <div class="copy">
            <span><span style="font-size: 30px;">&copy;</span> Mohamed Achouch</span>
        </div>
        </footer>
   
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
